<div class="page-header">
  <h1>Date sample data</h1>
</div>

<div class="row">
  <div class="col-xs-12">
    <h3>Date sample data</h3>
  </div>
  <% @date_data.each do |sample| %>
      <div class="col-xs-6">
        <strong><%= sample[:type] %></strong>: <%= sample[:value] %>
      </div>
  <% end %>
</div>

<div class="row">
  <div class="col-xs-12"><hr /></div>
</div>

<div class="row">
  <div class="col-xs-12">
    <h3>Time sample data</h3>
  </div>
  <% @time_data.each do |sample| %>
      <div class="col-xs-6">
        <strong><%= sample[:type] %></strong>: <%= sample[:value] %>
      </div>
  <% end %>
</div>
